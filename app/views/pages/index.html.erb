<div class="container is-fluid margins">
  <p class="header">Search Results</p>
  <% if @jobs.any? %>
    <div id="jobs">
      <% @jobs.each do |job| %>
        <%= render partial: 'layouts/getRankings', locals: { :job => job } %>
      <% end %>
    </div>
    <br>
    <%= will_paginate @jobs %>
  <% else %>
    <article class="message">
      <div class="message-body">
        No results yet.
      </div>
    </article>
  <% end %>
</div>
